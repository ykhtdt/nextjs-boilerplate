{
  "extends": "next/core-web-vitals",
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "indent": [
      "error",
      2
    ],
    "no-tabs": [
      "error"
    ],
    "semi": [
      "error",
      "never"
    ],
    "quotes": [
      "error",
      "double"
    ],
    "react/no-children-prop": "off",
    "react/no-unescaped-entities": "off"
  }
}
